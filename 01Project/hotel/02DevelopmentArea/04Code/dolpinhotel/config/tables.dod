SYSTEM_USER{
	ID INT +PK,
	NUMBER VARCHAR(20),
	PASSWORD VARCHAR(20),
	NAME VARCHAR(20),
	GENDER CHAR(2),
	AGE INT,
	BIRTHDAY CHAR(10),
	PHONE VARCHAR(20)
}

SYSTEM_ROLE{
	ID INT +PK,
	NAME VARCHAR(20),
	DESCRIPTION VARCHAR(500)
}

SYSTEM_USER_ROLE{
	ID INT +PK,
	USERID INT,
	ROLEID INT
}

SYSTEM_MENU{
	ID INT +PK,
	NAME VARCHAR(20),
	URL VARCHAR(200),
	DESCRIPTION VARCHAR(500),
	PARENTID VARCHAR(20),
	LEVEL INT
}

SYSTEM_ROLE_MENU{
	ID INT +PK,
	ROLEID INT,
	MENUID INT
}